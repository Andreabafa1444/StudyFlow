@model IEnumerable<StudyFlow.Models.Subject>

<h1>Subjects</h1>

<table>
    <tr>
        <th>Name</th>
        <th>Status</th>
        <th>Actions</th>
    </tr>

@foreach (var s in Model)
{
    <tr>
        <td>@s.Name</td>
        <td>@s.Status</td>
        <td>
            <a asp-action="Details" asp-route-id="@s.Id">Details</a>
        </td>
    </tr>
}
</table>
